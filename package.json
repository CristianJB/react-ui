{
  "name": "react-ui",
  "version": "0.0.1",
  "description": "React UI app",
  "main": "src/main.js",
  "scripts": {
    "postinstall": "npm run build",
    "server": "node start.js",
    "build": "webpack -p --progress --config webpack.production.config.js",
    "test": "nyc ava --fail-fast --verbose --require babel-register",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rm build/app.js",
    "start": "webpack-dev-server --config webpack-dev-server.config.js --progress --inline --colors",
    "release": "npm run clean && npm run build",
    "deploy": "cd build && git add --all && git commit -m \"Release at $(date)\" && git push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/barriquello/react-ui.git"
  },
  "keywords": [
    "skeleton",
    "template",
    "react",
    "quickstart"
  ],
  "author": "Carlos H. Barriquello",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/barriquello/react-ui/issues"
  },
  "homepage": "https://github.com/barriquello/react-ui",
  "devDependencies": {
    "autoprefixer": "^6.0.2",
    "ava": "^0.12.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.5.2",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.13.0",
    "babel-register": "^6.5.2",
    "coveralls": "^2.11.2",
    "css-loader": "^0.18.0",
    "enzyme": "^2.0.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "html-webpack-plugin": "^2.7.2",
    "json-loader": "^0.5.3",
    "nyc": "^5.6.0",
    "postcss-loader": "^0.6.0",
    "postcss-nested": "^1.0.0",
    "react-addons-test-utils": "^15.0.1",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.2.1",
    "react-router": "2.0.0",
    "react-router-redux": "^4.0.0-rc.1",
    "redux": "^3.2.1",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^1.0.3",
    "sinon": "^1.17.2",
    "style-loader": "^0.12.3",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "babel-core": "^6.5.2",
    "babel-polyfill": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "express": "^4.12.3",
    "material-ui": "^0.15.4",
    "rc-tree": "^1.3.9",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-flexbox-grid": "^0.10.2",
    "react-google-maps": "^4.11.0",
    "redux-devtools": "^3.1.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.3",
    "react-addons-shallow-compare": "^15.0.1",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.0"
  }
}
